{
    "version": 2,
    "functions": {
      "backend/index.js": {
        "runtime": "nodejs22.x",
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ],
    "ignoreCommand": "npm run build"
  }
  