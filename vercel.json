{
    "version": 2,
    "buildCommand": "npm install && npm run build",
    "functions": {
      "api/index.js": {
        "runtime": "nodejs18@22.x",
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ]
  }
  